{
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    }
  ],
  "outputDirectory": "dist",
  "includeFiles": [
    "server/**",
    "shared/**",
    "package.json",
    "tsconfig.json"
  ]
}