{
    "version": 2,
    "builds": [
      {
        "src": "server/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "source": "/api/(.*)",
        "destination": "/server/index.ts"
      },
      {
        "source": "/(.*)",
        "destination": "/dist/index.html"
      }
    ]
  }