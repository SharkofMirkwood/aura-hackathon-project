{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm ci --include=dev",
    "builds": [
      {
        "src": "server/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/public/index.html"
      }
    ]
  }