{
    "version": 2,
    "builds": [
      {
        "src": "dist/public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/public/index.html"
      }
    ],
    "buildCommand": "npm run build",
    "installCommand": "npm ci --include=dev",
    "outputDirectory": "dist/public"
  }